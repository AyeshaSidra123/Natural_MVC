@model IEnumerable<NatDMS.Models.EditProduct>

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" rel="stylesheet">
    <style>
        body {
            background-color: #f8f9fa; /* Set background color for the entire page */
        }
    </style>
</head>
<body>
    <div class="container mt-4 bg-white">
        <h2>Product Catalog</h2>

        <div class="row">
            @foreach (var product in Model)
            {
                <div class="col-sm-4 col-md-3 col-lg-2 mt-sm-2 mb-sm-3 mt-md-3 mb-md-4 mt-lg-4 mb-lg-5">
                    @*<img src="@product.PresignedUrl" alt="@product.Product1" class="img-fluid" />*@
                    <img src="@product.PresignedUrl" alt="ProfileImage" width="200" height="200" class="img-fluid" id="presignedurl" />
                    <p>@product.Category</p>
                    <p>@product.ProductName</p>
                </div>
            }
        </div>
    </div>

    <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js"></script>
</body>
</html>
